{
  "name": "@updatedev/ssr",
  "version": "0.2.2",
  "description": "Update SSR SDK",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./supabase": {
      "types": "./dist/types/providers/supabase/index.d.ts",
      "import": "./dist/esm/providers/supabase/index.js",
      "require": "./dist/cjs/providers/supabase/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsup",
    "build:declarations": "tsc -p tsconfig.declarations.json",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/updatedotdev/ssr.git"
  },
  "license": "MIT",
  "author": {
    "name": "Update Team",
    "email": "support@update.dev",
    "url": "https://github.com/updatedotdev"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "update",
    "sdk"
  ],
  "bugs": {
    "url": "https://github.com/updatedotdev/ssr/issues"
  },
  "homepage": "https://github.com/updatedotdev/ssr#readme",
  "devDependencies": {
    "@types/node": "^12.20.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "@updatedev/js": "^0.2.2",
    "cookie": "^1.0.2"
  }
}
